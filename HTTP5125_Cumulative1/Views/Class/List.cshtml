@model IEnumerable<HTTP5125_Cumulative1.Models.Class>


@{
    ViewBag.Title = "List";
}

<div id="containerlist">
    <h2>List of Classes</h2>
    <form action="/Class/List" method="GET">
        <input type="text" name="SearchKey" autcomplete="off" />
        <input type="submit" value="Search" />
    </form>
    @foreach (var Class in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Class/Show/@Class.ClassId"> @Class.ClassCode - @Class.ClassName  </a></div>
        </div>
    }
</div>